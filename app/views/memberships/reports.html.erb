

<h3>Busqueda</h3>
    <div class="row">
        <div class="col-sm-6">
          <%= form_tag "/search/createmembership", method: 'get', remote: true, id:"search-form" do %>

                <strong>Fecha de Afiliación: </strong><br><%= date_field_tag :keyword, nil %><br><br>

                <strong>Membresía: </strong><br><br>

                <%=select("",:keyword1,Society.all.collect {|p| [ p.nombre, p.id ] })%> <br><br><br>


                <%= content_tag :button,class:"btn btn-primary btn-lg" , type: :submit do%> Buscar <% end %>



            <% end %>
        </div>
      <div class="col-sm-6">
        <div id="graficasmembership">

        </div>
      </div>
    </div>

  <div class="container">
  <table class="table table-striped" width="300">
    <thead>
    <tr>
      <th width="80">Nombre </th>
      <th width="80">Documento</th>
      <th width="80">Periocidad</th>
      <th width="80">Fecha de Afiliación</th>
      <th width="80">Fecha de Vencimiento</th>
      <th width="80">Fecha Cuota</th>

      <th width="80">Sociedad</th>
    </tr>
    </thead>
  </table>
  <div id="resultadosmembership">
  </div>
</div>




